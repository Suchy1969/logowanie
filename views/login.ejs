<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link href="../styles/log-in.css" rel="stylesheet" />
    <title></title>
</head>
<body>
    <div class="menu">
        <div class="lewy-kontener">
            <a href="/" class="element-kontenera nazwa-strony">SKRZYNKI</a>
        </div>
        <div class="prawy-kontener">
            <div class="element-kontenera">Utwórz konto</div>
            <a href="log-in.html" class="element-kontenera">Zaloguj się</a>
        </div>
    </div>
    <div class="main-container">
        
        
        
        <div class="log-in-container">
                  <% if (hasMessages) { %>
            <ul>
            <% messages.forEach(function(message){ %>
              <li><%= message %></li>
            <% }); %>
            </ul>
          <% } %>
                <form action="/login/password" method="post">
            <input type="text" name="username" placeholder="Username">
            <input type="password" name="password" placeholder="Password">
            <button type="submit" class="contrast">Sign in</button></div>
    
    </div>


</body>
</html>